<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="rootBorderPane" prefHeight="563.0" prefWidth="749.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxml.gameTableController">
   <right>
      <VBox prefHeight="504.0" prefWidth="278.0" BorderPane.alignment="CENTER">
         <children>
            <ScrollPane fx:id="chatScrollPane" prefHeight="173.0" prefWidth="81.0">
               <content>
                  <FlowPane fx:id="flowPaneInScroll" prefHeight="129.0" prefWidth="200.0" />
               </content>
            </ScrollPane>
            <HBox prefHeight="27.0" prefWidth="219.0">
               <children>
                  <TextField fx:id="inGameChatTextField" onAction="#onEnter" />
                  <Button fx:id="inGameChatSendBtn" mnemonicParsing="false" onMousePressed="#inGameChatSend_ACTION" prefHeight="25.0" prefWidth="51.0" text="Send" />
               </children>
            </HBox>
            <AnchorPane fx:id="anchorConsole" prefHeight="171.0" prefWidth="278.0">
               <children>
                  <Label layoutX="57.0" layoutY="56.0" text="This is game console pane" />
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label layoutX="102.0" layoutY="82.0" text="this is for special cards pane" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </right>
</BorderPane>
